<?php

class manusia{

    var $nama;
    var $umur;
    var $jeniskelamin;
   
    function tampilkan_nama(){
        return "Nama : Noer Muhammad Abdian Ramdhanie <br/>";
    }
    
    function umur(){
        return "Umur : 16 tahun <br/>";
    }
    
    function jeniskelamin(){
      return "Jeniskelamin : Laki-laki <br/>";
    }
}

$manusia = new manusia();


echo $manusia->tampilkan_nama();


echo $manusia->umur();

echo $manusia->jeniskelamin();
?>
